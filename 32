#include <stdio.h>
int is_prime(int n) {
    if (n < 2) return 0;
    for (int i = 2; i*i <= n; i++)
        if (n % i == 0) return 0;
    return 1;
}
int main() {
    int count = 0;
    for (int i = 1; i <= 500; i++)
        if (is_prime(i)) count++;
    printf("Count = %d
", count);
    return 0;
}
