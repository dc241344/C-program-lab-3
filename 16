#include <stdio.h>
int main() {
    int n = 100, num, max, min;
    printf("Enter number 1: ");
    scanf("%d", &num);
    max = min = num;
    for (int i = 2; i <= n; i++) {
        scanf("%d", &num);
        if (num > max) max = num;
        if (num < min) min = num;
    }
    printf("Max = %d, Min = %d", max, min);
    return 0;
}
